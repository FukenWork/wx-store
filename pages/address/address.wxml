<view class="container">
<view class="modal-mask" bindtap="hideModal" catchtouchmove="preventTouchMove" wx:if="{{showModal}}"></view>
<view class="modal-dialog" wx:if="{{showModal}}">
  <view class="modal-content">
    是否确认删除。
  </view>
  <view class="modal-footer">
    <view class="btn-cancel" bindtap="onCancel" data-status="cancel">取消</view>
    <view class="btn-confirm" bindtap="onConfirm" data-status="confirm">确定</view>
  </view>
</view>
<view class="containers">
  <view wx:for="{{list}}" wx:key="index" class="box-content" wx:if="{{list.length !== 0}}"
    data-index="{{index}}">
    <view class="touch-item {{item.isTouchMove ? 'touch-move-active' : ''}}" wx:key>

      <view class="content">
      <view style="{{item.txtStyle}}" class='item' data-index='{{index}}' bindtouchstart="touchstart"
          bindtouchmove="touchmove" bindtouchend="touchE">
          <view class="listitem" catchtap="godetail" wx:key="index" data-key="{{item}}">
            <view class="item-summary-nomal">
              <view class="item-value">{{item.id}}
              </view>
            </view>
          </view>
        </view>
      </view>
      <view class="action-wrap">
        <view class="action del" data-id="{{item.id}}" catchtap="delBtn" data-item="{{item}}">
          <image
            src="data:image/png;base64,"
            class="del-icon"></image>
        </view>
      </view>
    </view>

  </view>
</view>
  <view class="empty-view" wx:if="{{ addressList.length <= 0 }}">
    <image class="icon"
      src="http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/icon-normal/noAddress-26d570cefa.png">
    </image>
    <text class="text">暂无数据，快去添加吧！</text>
  </view>
  <view class="add-address" bindtap="addressAddOrUpdate" data-address-id="0">新建</view>
</view>