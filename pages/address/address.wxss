.containers {
  overflow: hidden;
  width: 100%;
  position: relative;
}
.box-content {
  width: 100%;
  margin-bottom: 10rpx;
}
.touch-item {
  display:flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  background: #f5f5f5;
  margin-left: -28rpx;
  height: 200rpx;
}
  
.content {
  width: 100%;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(150rpx);
  transform: translateX(150rpx);
  margin-left: -120rpx;margin-top: 20rpx;
}
.action-wrap {
  position: relative;                           
}
 .action {
  width: 120rpx;
  display: flex;
  height: 200rpx;
  justify-content: center;
  align-items: center;
  background: #F15A5A;
  border-radius:10rpx;
  -webkit-transform: translateX(180rpx);
  transform: translateX(180rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  margin-top: 5rpx;
  margin-top: -90rpx;
}
.del-icon {
  width: 36rpx;
  height: 36rpx;
}
  
.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

page {
  background-color: #f8f8f8;
}

.history-page {
  line-height: 42rpx;
  font-weight: 400;
  padding-bottom: 37rpx;
  overflow: hidden !important;
}

.ppglist {
  margin-top: 10rpx;
  /* margin-bottom: 100rpx; */
}

.day-title {
  height: 42rpx;
  padding: 10rpx 50rpx;  
  font-size: 30rpx;
  color: rgba(68, 169, 179, 1);
  margin-bottom: 45rpx;
}

.listitem {
  display: flex;
  justify-content: space-between;
  background: #FFFFFF;
  border-radius: 10rpx;
  border: 1rpx solid rgba(223, 228, 228, 1);
  margin: 0rpx 36rpx 10rpx 36rpx;
  padding: 30rpx 30rpx 23rpx 30rpx;
  margin-top: -100rpx;
  height: 150rpx;
}

.item-result {
  height: 48rpx;
  font-size: 34rpx;
  line-height: 48rpx;
}

.item-value {
  margin-top: 18rpx;
  height: 71rpx;
  font-size: 60rpx;
  line-height: 71rpx;
}

.item-summary-nomal .item-result {
  color: rgba(64, 67, 67, 1);
}

.item-summary-nomal .item-value {
  color: rgba(68, 169, 179, 1);
}

.item-summary-exp .item-result {
  color: rgba(241, 90, 90, 1);
}

.item-summary-exp .item-value {
  color: rgba(241, 90, 90, 1);
}

.item-summary-less .item-result {
  color: rgba(200,203,204,1);
}

.item-summary-less .item-value {
  color: rgba(200,203,204,1);
}
.item-time {
  text-align: right;
  margin-top: 3rpx;
  color: rgba(161, 161, 161, 1);
  display: flex;
}

.item-arrow {
  width: 42rpx;
  height: 42rpx;
}

.unit {
  margin-left: 6rpx;
  height: 42rpx;
  font-size: 30rpx;
  color:#A1A1A1;  
}

.noData {
  width: 100%;
  height: 440rpx;
  margin: 200rpx auto;
  text-align: center;
}

.image {
  width: 440rpx;
  height: 440rpx;
}

.no_record {
  color: #404343;
  font-size: 50rpx;
  margin-top: 40rpx;
}

.tip {
  margin-top: 40rpx;
  font-size: 30rpx;
  color: #8a8a8f;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #3A4242;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -180rpx 105rpx;
  border-radius: 36rpx;
}

.modal-content {
  padding: 50rpx 32rpx;
  text-align: center;
  color: #404343;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 50%;
  color: #8A8A8F;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #44A9B3;
  text-align: center;
}